<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>target\companion-app-all.jar</jar>
  <outfile>target\ServicegestCompanion.exe</outfile>
  <errTitle>Servicegest Companion Error</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>https://adoptium.net/</downloadUrl>
  <supportUrl>https://servicegest.ro</supportUrl>
  <stayAlive>true</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest></manifest>
  <icon>icon.ico</icon>
  <singleInstance>
    <mutexName>ServicegestCompanionMutex</mutexName>
    <windowTitle>Servicegest - API Health Monitor</windowTitle>
  </singleInstance>
  <jre>
    <path>runtime</path>
    <bundledJre64Bit>true</bundledJre64Bit>
    <bundledJreAsFallback>false</bundledJreAsFallback>
    <minVersion>17</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>64</runtimeBits>
    <opt>--module-path "lib" --add-modules javafx.controls,javafx.fxml,javafx.graphics</opt>
  </jre>
  <versionInfo>
    <fileVersion>1.0.8.0</fileVersion>
    <txtFileVersion>1.0.8</txtFileVersion>
    <fileDescription>Servicegest API Health Monitor</fileDescription>
    <copyright>Â© 2025 Servicegest</copyright>
    <productVersion>1.0.8.0</productVersion>
    <txtProductVersion>1.0.8</txtProductVersion>
    <productName>Servicegest Companion</productName>
    <companyName>Servicegest</companyName>
    <internalName>ServicegestCompanion</internalName>
    <originalFilename>ServicegestCompanion.exe</originalFilename>
  </versionInfo>
</launch4jConfig>
